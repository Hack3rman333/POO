#include <iostream>
#include <vector>

using namespace std;

int main(){
    vector<int> v;
    vector<int> :: const_iterator it;
    cout << "v.size(" << v.size() << ")"  << endl;

        v.push_back(12);
        v.push_back(23);
        v.push_back(34);
        v.push_back(45);
        v.push_back(56);

    for(it = v.begin(); it != v.end(); ++it){
        cout << " | ";
        cout << *it << " | ";
        
    }
    cout << endl;

    cout << "v.size(" << v.size() << ")"  << endl;

    return 0;

}
